# Moomoo Connection Settings
MOOMOO_HOST=127.0.0.1
MOOMOO_PORT=11111

# Trading Environment - IMPORTANT: Start with SIMULATE!
TRD_ENV=SIMULATE
DRY_RUN=true

# Account Selection - Multiple Accounts Support
# Primary account (used if specific account IDs not provided)
ACC_ID=
ACC_INDEX=0

# Five Trading Accounts Configuration
# Account 1: Barbell Convex (Debit call vertical + Crash hedge)
ACC_ID_1=
ACC_INDEX_1=0

# Account 2: Balanced Income (Credit put spread)
ACC_ID_2=
ACC_INDEX_2=1

# Account 3: Volatility Bet (ATM straddle)  
ACC_ID_3=
ACC_INDEX_3=2

# Account 4: Wheel Strategy (Cash-secured puts)
ACC_ID_4=
ACC_INDEX_4=3

# Account 5: Mixed Small Bets (Alternating strategies)
ACC_ID_5=
ACC_INDEX_5=4

# Multi-account mode (true to use separate accounts for each strategy)
MULTI_ACCOUNT_MODE=false

# Strategy Parameters
UNIVERSE_MAX=6
TARGET_DELTA=0.30
SHORT_DELTA=0.20
WIDTH=5
CONTRACTS=1
DTE_TARGET=28
DTE_MIN=21
DTE_MAX=45

# Risk Management
WEEKLY_ADD_NZD=50
STARTING_NZD=10000
WEEKLY_SPEND_USD=150

# Rebalancing
REBALANCE_MONTHLY=true
REBALANCE_DOM=1

# Data & Logging
DATA_DIR=./data
LOG_FILE=./data/trading.log
FEE_LOOKBACK=50

# Backtesting
MIN_OI=500
MIN_VOLUME=1